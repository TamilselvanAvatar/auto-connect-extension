{
  "name": "IN Connect",
  "description": "IN Connect Extensions",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "in.jfif"
  },
  "permissions": [ "activeTab" ,"scripting"],
  "host_permissions" : ["<all_urls>"],
  "content_scripts" :[{
    "matches": ["<all_urls>"],
    "js" : ["content.js"]
  }]
}